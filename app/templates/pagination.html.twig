<div class="flex gap-2 justify-center p-5">
	{% if page > 0 %}
		<a href="{{ path(app_path, {'page': page - 1}) }}">
			<span>&#60;</span>-</a>
	{% endif %}
	{% for i in 0..pagesNb-1 %}

		<a href="{{ path(app_path, {'page': i, 'id':app.user.id}) }}" class="px-1 w-6 flex justify-center items-center {% if i == page %} rounded bg-cyan-600 text-white {% endif %}">{{ i + 1}}</a>
	{% endfor %}
	{% if page < pagesNb-1 %}
		<a href="{{ path(app_path, {'page': page + 1 , 'id':app.user.id}) }}">-></a>
	{% endif %}
</div>
